#include <iostream>
#include <set>
#include <string>
using namespace std;

int main() {
	set<set<string>> syn_voc;
	int Q;
	cin >> Q;
	while (Q > 0) {
		string queue;
		if (queue == "ADD") {
			string s1, s2;
			cin >> s1 >> s2;
			set<string> pair;
			pair = {s1, s2};
			syn_voc.insert(pair);
		}
		else if (queue == "COUNT") {
			string s;
			cin >> s;
			int count = 0;
			for (const auto& pair: syn_voc) {
				count += pair.count(s);
			}
			cout << count << endl;
		}
		Q--;
	}
	return 0;
}
