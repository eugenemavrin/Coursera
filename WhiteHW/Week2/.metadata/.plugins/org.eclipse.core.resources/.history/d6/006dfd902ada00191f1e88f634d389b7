#include <iostream>
using namespace std;


int myreverse(int n) {
	int res;
	while (n > 0) {
		res = (res + n % 10) * 10;
		n /= 10;
	}
	return res / 10;
}



int main() {
	int n, revn;
	cin >> n;
	revn = myreverse(n);

	if (myreverse(n) == n) {
		cout << "yeas" << endl;
	}
	else cout << "noo" << endl;

	cout << myreverse(n);

	return 0;
}
