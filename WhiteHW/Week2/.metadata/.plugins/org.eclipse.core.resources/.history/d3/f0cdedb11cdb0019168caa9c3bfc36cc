#include <iostream>
#include <vector>
#include <string>
using namespace std;

int main() {
	int n;
	cin >> n;
	vector<int> dim;
	dim = {31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31};
	int month = 0; //January;
	vector<vector<string>> tasks(0);
	while (n > 0) {
		string s;
		cin >> s;
		if (s == "ADD") {
			int day;
			cin >> day;
			string s;
			cin >> s;
			tasks[day].push_back(s);
			cout << 13;
		}
		else if (s == "DUMP") {
			int day;
			cin >> day;
			cout << tasks[day].size() << " ";
			for (auto task : tasks[day]) {
				cout << task << " ";
			}
		}
		else cout << "ERROR";
		n--;
	}
	return 0;
}
