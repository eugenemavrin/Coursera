#include <iostream>
#include <string>
#include <vector>
using namespace std;

bool IsPalindrom(string s) {
	string revs, tail;
	for (char c : s) {
		revs = c + tail;
		tail = revs;
	}

	return s == revs;
}

vector<string> PalindromFilter(vector<string> words, int minLength) {
	vector<string> res;
	int words;
	for (auto word : words) {
		if(IsPalindrom(word) && word.size() >= minLength)
			res.push_back(word);
	}

	return res;
}

int main() {
	string s;
	cin >> s;

	cout << IsPalindrom(s);
	return 0;
}
