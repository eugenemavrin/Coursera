#include <iostream>
#include <set>
#include <string>
#include <map>
using namespace std;

int main() {
	map<string, set<string>> syn_voc;
	int Q;
	cin >> Q;
	while (Q > 0) {
		string queue;
		cin >> queue;
		if (queue == "ADD") {
			string s1, s2;
			cin >> s1 >> s2;
			set<string> pair;
			pair.insert(s1);
			pair.insert(s2);
		}
		else if (queue == "COUNT") {
			string s;
			cin >> s;
			int count = 0;
			for (const auto& pair: syn_voc) {
				for (const string& si: pair) {
					if (s == si) {
						count++;
					}
				}
			}
			cout << count << endl;
		}
		else if (queue == "CHECK") {
			string s1, s2;
			cin >> s1 >> s2;
			set<string> pair;
			pair.insert(s1);
			pair.insert(s2);
			if(syn_voc.count(pair) > 0) {
				cout << "YES" << endl;
			}
			else cout << "NO" << endl;
		}
		Q--;
	}
	return 0;
}
