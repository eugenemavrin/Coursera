#include <iostream>
#include <vector>
using namespace std;

class Incognizable {
public:
	Incognizable() {
		_x = 0;
		_y = 0;
	};
	Incognizable(int x, int y) {
		_x = x;
		_y = y;
	}
private:
	int _x;
	int _y;
};
int main() {
  Incognizable a;
  return 0;
}

