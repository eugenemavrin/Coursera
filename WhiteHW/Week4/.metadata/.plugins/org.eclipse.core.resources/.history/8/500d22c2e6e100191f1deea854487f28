#include <iostream>
#include <fstream>
#include <string>
#include <iomanip>
using namespace std;

int main() {
	ifstream text("input.txt");
	int row, col;
	string s;
	//ofstream otext("output.txt");
	if(text.is_open()) {
		text >> row >> col;
		for (int i = 0; i < row; ++i) {
			for (int j = 0; j < col; ++j) {
				if (j < col - 1)
					getline(text, s, ',');
				else getline(text, s);

				int temp = stoi(s);
				cout << setw(10) << temp;
				if (j < col - 1)
					cout << " ";
			}
			if (i < row - 1)
				cout << endl;
		}
	}
	return 0;
}
