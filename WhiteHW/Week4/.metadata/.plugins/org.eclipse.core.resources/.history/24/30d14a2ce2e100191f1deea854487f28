#include <iostream>
#include <fstream>
#include <string>
#include <iomanip>
using namespace std;

int main() {
	ifstream text("input.txt");
	string s;
	//ofstream otext("output.txt");
	if(text.is_open()) {
		while (getline(text, s)) {
			//otext << s << endl;
			double d = stod(s);
			cout << fixed << setprecision(3);
			cout << d << endl;
		}
	}
	return 0;
}
