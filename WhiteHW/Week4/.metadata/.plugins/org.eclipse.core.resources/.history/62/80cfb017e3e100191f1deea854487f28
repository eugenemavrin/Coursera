#include <iostream>
#include <fstream>
#include <string>
#include <iomanip>
using namespace std;

int main() {
	ifstream text("input.txt");
	int s, row, col;
	//ofstream otext("output.txt");
	if(text.is_open()) {
		text >> col >> row;
		for (int i = 0; i < row; ++i) {
			for (int j = 0; j < col; ++j) {
				text >> s;
				cout << s;
				if (j < col - 1) {
					cout << " ";
				}
			}
			cout << endl;
		}
		while (row > 0) {
			cout << setw(10);
			cout << s << endl;
		}
	}
	return 0;
}
