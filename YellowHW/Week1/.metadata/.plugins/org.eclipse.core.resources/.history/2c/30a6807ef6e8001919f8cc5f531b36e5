#include <iostream>
#include <vector>

using namespace std;

int main() {
	int n = 0;
	cin >> n;
	vector<int> a(n);
	for (auto &d : a) {
		cin >> d;
	}
	int sum = 0;
	for (auto %d : a) {
		sum += d;
	}
	cout << sum / static_cast<int>a,size() << endl;
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
